//Armstrong number
#include <stdio.h>

int main() {
    int  c ,n,sum=0,rem;
    printf("enter the number:");
    scanf("%d",&n);
    c=n;
    while(n>0)
    {
        rem = n%10;
        sum = sum+(rem*rem*rem); 
        n = n/10;
    }
    if(c==sum)
    {
        printf("amstrong number");
    }
    else{
    printf("not a Armstrong number");
    }
    return 0;
}
