#include <stdio.h>
int main()
{
    int a[10],b[10],c[10],i,j=0,k=0,n;
    printf("enter the size:");
    scanf("%d",&n);
    printf("enter the elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
        {
            b[j]=a[i];
            j++;
        }
        else
        {
            c[k]=b[i];
            k++;
        }
    
    }
    for(i=0;i<k;i++)
    {
        printf("%d",c[i]);
    }
    return 0;
}
